<h1>hy</h1>

<%= form_for :user, url: '/create' do |f| %>
  First name: <%= f.text_field :name %><br />
  password : <%= f.password_field :password %><br />
  email : <%= f.text_field :email %><br />
  mobile : <%= f.text_field :mobile %><br />
  file : <%= f.file_field(:file) %><br />
  <%= f.submit %>
<% end %>